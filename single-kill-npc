-- note for oooga booga. this is the single kill npc. dont switch up the two stupid fuck

getgenv().player = game:GetService("Players").LocalPlayer
getgenv().fff = false
getgenv().Mouse = player:GetMouse()
getgenv().goofyahhnpc = nil

game:GetService("RunService").RenderStepped:Connect(function()
  if fff == true then

            local tool = player.Character and player.Character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("Handle") then
                tool:Activate()
    for i,ihateblackies in next, goofyahhnpc:GetChildren() do
                    if ihateblackies:IsA("BasePart")  then
                        firetouchinterest(tool.Handle,ihateblackies,0)
                        firetouchinterest(tool.Handle,ihateblackies,1)
                    end
                end
            end   
     
     end



end)
Mouse.KeyDown:connect(function(key)
if key == "z" then
if Mouse.Target and 
 
Mouse.Target.Parent:FindFirstChildWhichIsA("Humanoid") then
 for i,wewewe in next, Mouse.Target.Parent:GetChildren() do
     if wewewe:IsA("Humanoid") and wewewe.Health > 0 then
    getgenv().goofyahhnpc = Mouse.Target.Parent
    getgenv().zzzz = wewewe.Parent
    getgenv().humanoidhealth = wewewe.Health
    getgenv().fff = true
                           while fff == true do
wait()
mouse1click()
end
getgenv().ligger = true
while ligger == true do
    wait()
    if humanoidhealth > 0 or player.Character:FindFirstChild("Humanoid").Health > 0 then
        getgenv().fff = false
        wait()
        getgenv().ligger = false
                           while fff == true do
wait()
mouse1click()
end
end
end
end
end
end
end
end)
Mouse.KeyDown:connect(function(key)
if key == "x" then
          getgenv().fff = false
        wait()
        getgenv().ligger = false
end
    end)
while true do
    wait(0.3)
       for i,normal in next, game.Workspace:GetDescendants() do

                    if normal:IsA("Accessory")  then
normal:Destroy()
                    end
   
       end
            end
