  game.Loaded:Wait()  while true do wait() spawn(function() if game.Players.LocalPlayer.Character.Humanoid.RigType == "R15" then for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do if	v.Name == "RightUpperLeg" then v:Destroy() end end else for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do if	v.Name == "Right Leg" then v:Destroy() end end end end) end
