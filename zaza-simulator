c:Button("fake afk",function()
local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").AFK:FireServer(unpack(args))

end)
c:Button("fake afk off",function()
local args = {
    [1] = false
}

game:GetService("ReplicatedStorage").AFK:FireServer(unpack(args))

end)
